{
  "name": "reinforce-ts",
  "version": "1.1.3",
  "description": "Library of Reinforcement Learning Agents in Javascript (baked with Typescript). Ported, object-oriented and refactored version of Andrej Karpathy's reinforce-js (https://github.com/karpathy/reinforcejs)",
  "keywords": [
    "reinforcement learning",
    "q-learning",
    "sarsa",
    "temporal difference",
    "deterministic policy gradients",
    "recurrent",
    "agent",
    "neural net",
    "solver"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prepare": "tsc -p .",
    "pretest": "npm run compile",
    "test": "node"
  },
  "author": "Menno van Rahden",
  "homepage": "https://github.com/mvrahden/reinforce-ts#readme",
  "bugs": {
    "url": "https://github.com/mvrahden/reinforce-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvrahden/reinforce-ts.git"
  },
  "license": "MIT",
  "devDependencies": {
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "recurrent-ts": "^1.2.1"
  }
}
