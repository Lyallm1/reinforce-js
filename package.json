{
  "name": "reinforce-js",
  "version": "1.4.0",
  "description": "Library of Reinforcement Learning Agents in Javascript (baked with Typescript). Ported, object-oriented and refactored version of Andrej Karpathy's reinforce-js (https://github.com/karpathy/reinforcejs)",
  "keywords": [
    "reinforcement learning",
    "q-learning",
    "sarsa",
    "temporal difference",
    "deterministic policy gradients",
    "recurrent",
    "agent",
    "neural net",
    "solver"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "test": "./node_modules/.bin/jasmine",
    "pretest": "npm run build:test",
    "build:test": "tsc -p ./tsconfig.spec.json",
    "prebuild:test": "npm run build",
    "build": "tsc -p .",
    "prebuild": "rimraf dist & rimraf spec/dist",
    "posttest": "rimraf spec/dist"
  },
  "author": "Menno van Rahden",
  "homepage": "https://github.com/mvrahden/reinforce-js#readme",
  "bugs": {
    "url": "https://github.com/mvrahden/reinforce-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvrahden/reinforce-js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.4",
    "jasmine": "^2.8.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "recurrent-js": "^1.4.6"
  }
}
